{
  "name": "infoexam/password",
  "description": "Infoexam user provider.",
  "type": "library",
  "homepage": "https://github.com/Infoexam/password",
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/auth": "^5.3",
    "illuminate/events": "^5.3",
    "illuminate/hashing": "^5.3",
    "illuminate/support": "^5.3"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.3",
    "illuminate/database": "^5.3",
    "orchestra/testbench": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Infoexam\\Password\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "notify-on-install": false
  }
}
